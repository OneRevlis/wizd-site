{
  "rewrites": [
    {
      "source": "/contact",
      "destination": "/contact/page.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy/page.html"
    },
    {
      "source": "/terms-conditions",
      "destination": "/terms-conditions/page.html"
    },
    {
      "source": "/404",
      "destination": "/index.html"
    },
    {
      "source": "/((?!cdn-cgi).*)",
      "destination": "/index.html"
    }
  ]
}

